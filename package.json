{
  "name": "micropython-esp32",
  "displayName": "MicroPython-ESP32",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:micropython-esp32.connectESP32",
    "onCommand:micropython-esp32.executePythonCommand",
    "onCommand:micropython-esp32.disconnectESP32"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "micropython-esp32.connectESP32",
        "title": "Connect to ESP32"
      },
      {
        "command": "micropython-esp32.executePythonCommand",
        "title": "Execute Python Command"
      },
      {
        "command": "micropython-esp32.disconnectESP32",
        "title": "Disconnect to ESP32"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "micropython-esp32.connectESP32",
          "when": "!micropython-esp32.connected"
        },
        {
          "command": "micropython-esp32.executePythonCommand",
          "when": "micropython-esp32.connected"
        },
        {
          "command": "micropython-esp32.disconnectESP32",
          "when": "micropython-esp32.connected"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "serialport": "^10.4.0",
    "socket.io-client": "^4.4.1"
  }
}
